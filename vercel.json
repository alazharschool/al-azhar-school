{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "app/blog/arabic-language/page.tsx": {
      "maxDuration": 30
    }
  },
  "regions": ["iad1"],
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false
} 